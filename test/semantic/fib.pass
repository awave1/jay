main() {
        int i;
        i = 0;

        //  Anything larger than 47 overflows an int..
        //
        while (i <= 47) {
                prints("fib(");
                printi(i);
                prints(") = ");
                printi(fib(i));
                prints("\n");
                i = i + 1;
        }
}

int fib(int n) {
        if (n == 0) return 0;
        if (n == 1) return 1;
        return fib(n-1) + fib(n-2);
}